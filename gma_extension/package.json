{
	"publisher": "aol27cuew27wkrbzzq6djpcii6wqib6ejy3y56y2lfp2yspeiyyq",
	"private": true,
	"name": "gma",
	"displayName": "GMA Extensions",
	"description": "GMA Extensions",
	"version": "0.0.1",
	"icon": "images/koyal_icon.png",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/gma.yaml"
	],
	"main": "./out/extension.js",
	"contributes": {
		"taskDefinitions": [
			{
				"type": "dart",
				"required": [
					"flavor",
					"app"
				],
				"properties": {
					"flavor": {
						"type": "string",
						"description": "Short flavor tag Indian Fake [fakein]"
					},
					"app": {
						"type": "string",
						"description": "App folder Customer [capp]"
					}
				}
			}
		],
		"configurationDefaults": {},
		"configuration": [
			{
				"title": "GMA Studio",
				"type": "object",
				"properties": {
					"gma.flavor.apps": {
						"type": "array",
						"items": {
							"type": "object"
						}
					},
					"gma.flavor.stages": {
						"type": "array",
						"items": {
							"type": "object"
						}
					},
					"gma.flavor.countries": {
						"type": "array",
						"items": {
							"type": "object"
						}
					}
				}
			}
		],
		"commands": [
			{
				"command": "gma.flavor.showBoard",
				"title": "Show Cat Codicons",
				"category": "Cat Codicons"
			},
			{
				"category": "GMA",
				"command": "gma.flavor.changeApp",
				"title": "Select application"
			},
			{
				"category": "GMA",
				"command": "gma.flavor.changeCountry",
				"title": "Select country"
			},
			{
				"category": "GMA",
				"command": "gma.flavor.changeStage",
				"title": "Select stage"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "gma-activity",
					"title": "GMA Tools",
					"icon": "images/koyal_icon.svg"
				}
			]
		},
		"views": {
			"gma-activity": [
				{
					"id": "nodeDependencies",
					"name": "Documentation",
					"icon": "images/koyal_icon.svg",
					"contextualTitle": "Documentation"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "nodeDependencies",
				"contents": "[Widget Catalog](command:gma.flavor.showBoard)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"@vscode/codicons": "0.0.21",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.4.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"i18n-iso-countries": "^6.8.0"
	}
}
