{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": [
			"es6"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"importHelpers": true,
		"forceConsistentCasingInFileNames": true
	},
	"exclude":[
		".vscode",
		".vscode-test",
		"node_modules"
	],
	"include": ["src/**/*.ts", "test/**/*"],
}
