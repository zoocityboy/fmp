{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "dist",
		"lib": [
			"es2019"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"importHelpers": true,
		"forceConsistentCasingInFileNames": true,
		
	},
	"exclude":[
		".vscode",
		".vscode-test",
		"node_modules"
	],
	"include": ["src/**/*", "test/**/*"],
}
