{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2020",
		"outDir": "dist",
		"lib": [
			"es2020"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"importHelpers": true,
		"forceConsistentCasingInFileNames": true,
		"experimentalDecorators": true
	},
	"exclude":[
		".vscode",
		".vscode-test",
		"node_modules"
	],
	"include": ["src/**/*", "test/**/*"],
}
